<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<jar-name>onbase-export.jar</jar-name>
	<plugin-name>ONBASE_EXPORT_PLUGIN</plugin-name>
	<plugin-desc>OnBase Export Plugin</plugin-desc>
	<plugin-workflow-name>OnBase_Export_Plugin</plugin-workflow-name>
	<plugin-service-instance>OnBasePlugin</plugin-service-instance>
	<method-name>execute</method-name>
	<is-scripting>FALSE</is-scripting>
	<back-up-file-name>OnBaseExport</back-up-file-name>
	<script-name>N/A</script-name>
	<application-context-path>applicationContext-onbase-export.xml</application-context-path>
	<plugin-properties>
		<plugin-property>
			<name>SHARED_FOLDER_PATH</name>
			<type>STRING</type>
			<description>Batch data folder share</description>
			<is-mandatory>TRUE</is-mandatory>
			<is-multivalue>FALSE</is-multivalue>
		</plugin-property>
		<plugin-property>
			<name>FINAL_DROP_FOLDER_PATH</name>
			<type>STRING</type>
			<description>Ephesoft final drop folder path</description>
			<is-mandatory>TRUE</is-mandatory>
			<is-multivalue>FALSE</is-multivalue>
		</plugin-property>
		<plugin-property>
			<name>SYSTEM_BATCH_FOLDER_PATH</name>
			<type>STRING</type>
			<description>Ephesoft system folder</description>
			<is-mandatory>TRUE</is-mandatory>
			<is-multivalue>FALSE</is-multivalue>
		</plugin-property>
		<plugin-property>
			<name>DIP_FOLDER</name>
			<type>STRING</type>
			<description>OnBase DIP folder</description>
			<is-mandatory>TRUE</is-mandatory>
			<is-multivalue>FALSE</is-multivalue>
		</plugin-property>
	</plugin-properties>
	<dependencies>
	</dependencies>
</plugin>